{
  "name": "ava-slack-bot",
  "registry": "imageintelligence",
  "run": "python bot/run.py",
  "packer": "./packer/",
  "env": {
    "CUSTOM_ENVIRON_VAR_1": "everything",
    "CUSTOM_ENVIRON_VAR_2": "is",
    "CUSTOM_ENVIRON_VAR_3": "awesome"
